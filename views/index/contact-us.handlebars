<div class="container-fluid">
    {{> _banner_sub}}
   
    <div class="container" style="margin-top: 250px">
        <h3 class="header-text-01">
            {{ contact.title}}
        </h3>
        <div class="hr-1px"></div>
        <div class="row">
            <div class="col s12 m8 l8">
                <p class="header-text-01">
                    {{{ contact.desc }}}
                </p>

                <div class="row">
                    <div class="input-field col s12">
                        {{#if errors}}
                        <div class="card-panel red lighten-4">
                            {{#each errors}} {{error}}
                            <br> {{/each}}
                        </div>
                        {{/if}} {{#each successMsg}}

                        <div class="card-panel green lighten-4">
                            <h3>
                                <p style="text-align: center"> {{thankYou}} </p>
                            </h3>
                            <p style="text-align: center">{{success}}</p>
                        </div>
                        {{/each}}
                    </div>
                </div>

                <div class="row">
                    <img src="/images/{{ contact.imgUrl }}" alt="{{ contact.imgUrl }}" class="responsive-img">
                </div>
            </div>

            <div class="col s12 m1 l1"></div>

            <div class="col s12 m3 l3">

               

                <div class="row header-text-01">
                    <h5>Phone</h5>
                    <div class="hr-1px-p10"></div>
                    <div class="col s12 m2 l2">
                        <p>
                            <i class="material-icons">call</i>
                        </p>
                    </div>
                    <div class="col s12 m10 l10">
                        <p>
                            <a class="blue-grey-text" href="tel:{{ contact.phone }}">{{ contact.phone }}</a>
                        </p>
                    </div>

                </div>



                <div class="row header-text-01">
                    <h5>Email</h5>
                    <div class="hr-1px-p10"></div>

                    <div class="col s12 m2 l2">
                        <p>
                            <i class="material-icons">drafts</i>
                        </p>
                    </div>
                    <div class="col s12 m10 l10">
                        <p>
                            <a class="blue-grey-text" href="mailto:{{ contact.email }}?Subject=Enquiry from samuioceantour.com">{{ contact.email }}</a>
                        </p>
                    </div>

                </div>

                {{!-- <div class="row header-text-01">
                    <h5>WhatsApp</h5>
                    <div class="hr-1px-p10"></div>

                    <div class="col s12 m2 l2">
                        <p>

                            <i class="material-icons">call</i>

                        </p>
                    </div>
                    <div class="col s12 m10 l10">
                        <p>
                            <a class="blue-grey-text" href="#">{{ contact.whatsapp }}</a>
                        </p>

                    </div>


                </div> --}}

                {{!-- <div class="row header-text-01">
                    <h5>Skype</h5>
                    <div class="hr-1px-p10"></div>
                    <div class="col s12 m2 l2">
                        <p>
                            <i class="material-icons">call</i>
                        </p>
                    </div>
                    <div class="col s12 m10 l10">
                        <p> <a class="blue-grey-text" href="#">{{ contact.skype }}</a>
                        </p>
                    </div>

                </div> --}}

                <div class="row header-text-01">
                    <h5>Address</h5>
                    <div class="hr-1px-p10"></div>
                    <div class="col s12 m2 l2">
                        <p>
                            <i class="material-icons">location_on</i>
                        </p>
                    </div>
                    <div class="col s12 m10 l10">
                        <p>
                            <p class="blue-grey-text" href="#">
                            {{ contact.addr1 }}
                            <br>
                            <span>{{ contact.addr2 }}</span>
                            <br>
                            </p>
                        </p>
                    </div>
                </div>

                {{#if contact.lat }}
                <div class="row header-text-01">
                    {{!-- <h5>Maps</h5> --}}
                    
                    {{> _maps-contact }}
                </div>
                {{/if}}
            </div>
        </div>
    </div>
</div>