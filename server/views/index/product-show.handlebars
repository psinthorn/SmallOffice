                <img class="responsive-img" src="/images/product/{{ product.imgUrl }}" style="height: 400px;">                               
            </p>
            </div>                
           <div class="card">
               {{> _product-gallery }}
           </div>
             
        </div>

        <div class="col s1 m1 l1 hidden-on-xs"></div>

        <div class="col s12 m6 l6">
            <h4 class="header-text-01">
                {{ product.title }}
        </h4>
   
            <p class="header-text-01">
                {{{ product.desc }}}
            </p>

            <br>  
        </div>         
         

        </div>

        <div class="row">
            <div class="col s12 m6 l6">
                {{> _product-include }} 
            
            </div>

             <div class="col s12 m6 l6">
               
                {{> _product-exclude }}
            </div>
        </div>

